<div class="container mt-4 mb-4">
  <h2><span class="admin_title">管理画面：ユーザー一覧情報</span></h2>
    <div class="col-md-10 mx-auto pt-3">
      <table class="table">
        <thead class="thead-light">
          <tr>
            <th>id</th>
            <th>ユーザー名</th>
            <th>登録日</th>
          </tr>
        </thead>
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <td><%= user.created_at %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
</div>
