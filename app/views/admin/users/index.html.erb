<div class="container">
  <h2>管理画面：ユーザー一覧情報</h2>
  <table>
    <thead>
      <tr>
        <th>id</th>
        <th>ユーザー名</th>
        <th>登録日</th>
      </tr>
    </thead>
    <% @users.each do |user| %>
    <tbody>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.created_at %></td>
      </tr>
    </tbody>
  </table>
</div>
