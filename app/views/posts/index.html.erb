  <div class="row">
    <% @post.each do |post| %>
    <div class="col-3 p-0 ml-5">
    <% if post.photo.attached? %>
    <%= link_to post_path(post.id) do %>
    <%= image_tag post.photo, size: "320x300" %>
    <% end %>
    <% end %>
    <%= post.title %>
    <%#= image_tag post.user.profile_image %>
    <%= post.user.name %><br>
    難易度:
    </div>
    <% end %>
  </div>